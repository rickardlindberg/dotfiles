#!/usr/bin/env bash

exec find . \
    -type d -name .git        -prune -o \
    -type d -name .hg         -prune -o \
    -type d -name __pycache__ -prune -o \
    -type d -name venv        -prune -o \
    -type d -name .cache      -prune -o \
    -type f -name '.*swp'     -prune -o \
    -type f -name '*pyc'      -prune -o \
    -type f -print
